<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://homecredit.net/homer/ws/contract"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:cntr="http://homecredit.net/homer/ws/contract"
           xmlns:user="http://homecredit.net/homer/ws/user"
           xmlns:prod="http://homecredit.net/homer/ws/contract/product"
           xmlns:codb="http://homecredit.net/homer/ws/codebook"
           xmlns:pers="http://homecredit.net/homer/ws/person"
           xmlns:cont="http://homecredit.net/homer/ws/contact"
           xmlns:cmmn="http://homecredit.net/homer/ws/common"
           xmlns:cd="http://homecredit.net/homer/ws/card"
           xmlns:good="http://homecredit.net/homer/ws/goods"
           xmlns:rpt="http://homecredit.net/homer/ws/report"
           xmlns:addr="http://homecredit.net/homer/ws/address"
           xmlns:slp="http://homecredit.net/homer/ws/sellerplace">
    <xs:import namespace="http://homecredit.net/homer/ws/person" schemaLocation="Person.xsd"/>


    <xs:annotation>
        <xs:documentation>Version 1.3</xs:documentation>
    </xs:annotation>

    <xs:element name="ContractStatusInQuorum" type="cntr:ContractStatusInQuorum"/>
    <xs:simpleType name="ContractStatusInQuorum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="NEW"/>
            <xs:enumeration value="ACTIVE"/>
            <xs:enumeration value="CLOSED"/>
            <xs:enumeration value="SECURIZED"/>
            <xs:enumeration value="SOLD"/>
            <xs:enumeration value="BLOCKED"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SupplementAction">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CREATE"/>
            <xs:enumeration value="SIGN"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="QuickNoResultEnum" type="cntr:QuickNoResultEnum"/>
    <xs:simpleType name="QuickNoResultEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CONTINUE"/>
            <xs:enumeration value="REJECTED"/>
            <xs:enumeration value="IN_PROCESS"/>
            <xs:enumeration value="CHECK_DATA"/>
            <xs:enumeration value="FILL_ADDRESS"/>
            <xs:enumeration value="FILL_DATA"/>
            <xs:enumeration value="APPROVAL"/>
            <xs:enumeration value="ERROR"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="AvailableProductsEnum" type="cntr:AvailableProductsEnum"/>
    <xs:simpleType name="AvailableProductsEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SPECIAL"/>
            <xs:enumeration value="ZERO_OVERDRAFT_CARD"/>
            <xs:enumeration value="HAS_CARD"/>
            <xs:enumeration value="MFO_ONLY"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BlazeValueCodeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="annuityHCFB">
                <xs:annotation>
                    <xs:documentation>Total amount of monthly payments in HCFB for the client
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="annuityOther">
                <xs:annotation>
                    <xs:documentation>Total amount of monthly payments in other banks (not in HCFB)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="clientType">
                <xs:annotation>
                    <xs:documentation>Client type in Blaze (n - new client, a - active existing client)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxCreditSCAmount"/>
            <xs:enumeration value="maxCreditRDAmount"/>
            <xs:enumeration value="needFullApplication"/>
            <xs:enumeration value="maxCreditRFAmount"/>
            <xs:enumeration value="maxCreditCOLAmount">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма Кредита КН по залогам без подтверждения дохода</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxCreditSCAmountICN">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма Кредита КН с подтверждением дохода (Income Confirmation Needed)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxCreditRDAmountICN">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма Кредита РК с подтверждением дохода (Income Confirmation Needed)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxCreditRFAmountICN">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма Кредита Рефинансирования с подтверждением дохода (Income Confirmation Needed)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxCreditCOLAmountICN">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма Кредита КН по залогам с подтверждением дохода (Income Confirmation Needed)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="maxLoanMCCAmount">
                <xs:annotation>
                    <xs:documentation>Максимальная сумма займа МКК без подтверждения дохода</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="ExistContractEnum" type="cntr:ExistContractEnum"/>
    <xs:simpleType name="ExistContractEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="RC_CONTRACT"/>
            <xs:enumeration value="TRIO_CONTRACT"/>
            <xs:enumeration value="NEW_CONTRACT"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="QuickNoReq" type="cntr:QuickNoReq"/>
    <xs:complexType name="QuickNoReq">
        <xs:sequence>
            <xs:element name="originator" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="sellerplaceCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="userCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
            <xs:element name="quickNoApplicationNumber" type="xs:long" nillable="true" minOccurs="1" maxOccurs="1"/>
            <xs:element name="birthDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
            <xs:element name="birthPlace" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="contact" type="cont:Contact" nillable="true" minOccurs="1" maxOccurs="1"/>
            <xs:element name="document" type="pers:Document" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="internalCode" type="xs:string" nillable="true" minOccurs="1" maxOccurs="1"/>
            <xs:element name="name" type="pers:PersonName" minOccurs="1" maxOccurs="1"/>
            <xs:element name="registrationAddress" type="pers:PersonAddress" minOccurs="0" maxOccurs="1"/>
            <xs:element name="creditType" type="prod:CreditType" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="QuickNoResult" type="cntr:QuickNoResult"/>
    <xs:complexType name="QuickNoResult">
        <xs:sequence>
            <xs:element name="quickNoApplicationNumber" type="xs:long" minOccurs="1" maxOccurs="1"/>
            <xs:element name="availableProducts" type="cntr:AvailableProductsEnum" minOccurs="0" maxOccurs="1"/>
            <xs:element name="cuid" type="xs:long" minOccurs="0" maxOccurs="1"/>
            <xs:element name="newCustomer" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            <xs:element name="quickNoContractNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="quickNoResult" type="cntr:QuickNoResultEnum" minOccurs="1" maxOccurs="1"/>
            <xs:element name="checkDataDetail" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="blazeValue" type="cntr:BlazeValue" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="rejectReasonCode" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Optional
                        reject reason code, filled if quickNoResult = reject
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rejectReason" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Optional
                        name of reject reason. filled for corresponding rejectReasonCode
                        see param.registers, reg_number = 401.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:element name="BlazeValue" type="cntr:BlazeValue"/>
    <xs:complexType name="BlazeValue">
        <xs:sequence>
            <xs:element name="code" type="cntr:BlazeValueCodeEnum">
                <xs:annotation>
                    <xs:documentation>Code of parameter</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="xs:string" nillable="true">
                <xs:annotation>
                    <xs:documentation>Value for passed code</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <xs:element name="ApplicationResult" type="cntr:ApplicationResult"/>
    <xs:complexType name="ApplicationResult">
        <xs:complexContent>
            <xs:extension base="cntr:ContractResult">
                <xs:sequence>
                    <xs:element name="offers" type="cntr:ContractOffers" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="boxContractNumber" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Номер договора страхования
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="OffersAppliacation" type="cntr:OffersAppliacation"/>
    <xs:complexType name="OffersAppliacation">
        <xs:complexContent>
            <xs:extension base="cntr:ApplicationData">
                <xs:sequence>
                    <xs:element name="scoringTypeCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="calcPreference" type="cntr:OffersCalcPreference" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Class represents preferences, which is used for calculation of offers in
                                case of dynamic scoring
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="OfferCalcInsurancePreference" type="cntr:OfferCalcInsurancePreference"/>
    <xs:complexType name="OfferCalcInsurancePreference">
        <xs:sequence>
            <xs:element name="paidByLoan" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Признак предпочтения по оплате за счет кредита или за счет собственных средств

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceCompany" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Наименование страховой компании, если у клиента есть предпочтение по страховой компании. Поле
                        является необязательным, при незаполненном значении считаем, что у клиента нет предпочтений по
                        страховой компании

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insuranceGroup" type="prod:InsuranceGroup" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Признак группы типа страхования (страхование жизни, страхование от потери работы)

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="OffersCalcPreference" type="cntr:OffersCalcPreference"/>
    <xs:complexType name="OffersCalcPreference">
        <xs:complexContent>
            <xs:extension base="prod:CalculationPreference">
                <xs:sequence>
                    <xs:element name="channelCode" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Mandatory in case, that productCode is not filled

                                Meaning
                                Channel code

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="productCode" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Product code

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="productRate" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Restriction on product rate

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="paymentNumMax" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Restriction to number of payment, maximum

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="paymentMax" type="xs:double" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Restriction to high of regular payment

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="initPayMax" type="xs:double" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Restriction to initial payment, max.

                                Other
                                SS contracts
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="quickProcess" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Quick verification process

                                Other
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="priority" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Mandatory

                                Meaning
                                Information according which parameter should be offers ordered

                                Other

                                PP - Number of payment
                                HP - Payment in cash
                                VP - Payment amount
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="insuranceGoodsGroup" type="xs:long" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Mandatory

                                Meaning
                                Information about goods insurance

                                Other
                                SS contracts
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="offerCalcInsurancePreference" type="cntr:OfferCalcInsurancePreference"
                                minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Optional/Mandatory
                                Optional

                                Meaning
                                Атрибут предпочтения по страховке в рамках динамического скоринга.

                                Other
                                На каждую группу типов страхования должно быть не более одного объекта
                                OfferCalcInsurancePreference в списке
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="Offer" type="cntr:Offer"/>
    <xs:complexType name="Offer">
        <xs:sequence>
            <xs:element name="offerNum" type="xs:int" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Identification number of an offer

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="offerType" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Type of an offer.

                        Other
                        REG 406

                        - alo - Alternative offer
                        - dso - Dynamic scoring offer
                        - dro - Debt restructuring offer
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="productSet" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Code of the product set containing the offered product at the given sellerplace
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractNumber" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Contract number.

                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="creditAmount" type="xs:double" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        High of credit, which is offered.

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="price" type="xs:double" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Optional

                        Meaning
                        Price of goods

                        Other
                        Mandatory for SS contracts
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="initPay" type="xs:double" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Optional

                        Meaning
                        Initial payment

                        Other
                        Mandatory for SS contract
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="payment" type="xs:double" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        The high of regular payment

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentFees" type="xs:decimal" minOccurs="0" maxOccurs="1" >
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Optional

                        Meaning
                        The sum of all monthly fees

                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentNum" type="xs:int" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        The number of payments for a credit

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sendStatement" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Information whether statement service is required

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Status of an offer

                        Other

                        - a - active
                        - n - inactive
                        - s - choosen
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="insurance" type="prod:Insurance" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The set of insurances, which are offered</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="product" type="prod:Product" minOccurs="1" maxOccurs="1"/>
            <xs:element name="validTo" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Date until which the offer is valid</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="approvedContractNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="maxOuterLimit" type="xs:double" minOccurs="0" maxOccurs="1"/>
            <xs:element name="versionRdTw" type="xs:int" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="Contract" type="cntr:Contract"/>
    <xs:complexType name="Contract">
        <xs:sequence>
            <xs:element name="contractNumber" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory

                        Meaning
                        Contract number

                        Other
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateReview" type="xs:date" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Date when contract was created (moving contract into r status)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="creditAmount" type="xs:double" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Credit amount of a contract</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="price" type="xs:double" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Price of goods. Mandatory for SS contracts</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="payment" type="xs:double" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Regular high of payment number</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="commision" type="xs:double" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>High of commission on a contract</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="interestRate" type="xs:double" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Interest rate of a contract</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateSign" type="xs:date" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Date of signing of contract</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Status of a contract related to codebook REG 15</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="accessStamp" type="user:AccessStamp" minOccurs="1" maxOccurs="1"/>
            <xs:element name="product" type="codb:CodeItem" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Class returns information about product in sense of Code and name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="productSet" type="codb:CodeItem" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Information about product set (Code name)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sellerplace" type="codb:CodeItem" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Unique code of sellerplace</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="creditType" type="prod:CreditType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Type of credit (SS, SC...)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="processStage" type="prod:ProcessStage" minOccurs="1" maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>Optional/Mandatory
                        Mandatory
                        Stage of approval</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rateNum" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Number of current airbank rate or null if contract doesn't belong to airbank product</xs:documentation>
                </xs:annotation>
            </xs:element>
